---
<% if $username != '' { -%>
:username: '<%= $username %>'
<% } -%>
<% if $password != '' { -%>
:password: '<%= $password %>'
<% } -%>
<% if $protocol != '' { -%>
:protocol: '<%= $protocol %>'
<% } -%>
<% if $host != '' { -%>
:host: '<%= $host %>'
<% } -%>
<% if $port != '' { -%>
:port: <%= $port %>
<% } -%>
<% if $debug == 'true' { -%>
:debug: True
<% } -%>
<% if $debug == 'false' { -%>
:debug: False
<% } -%>
